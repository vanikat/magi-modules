
groups:
  agent_group: [control]
    
agents:
  c_agent:
    group: agent_group
    path: /users/vanikat/magi-modules/cagent/ 
    execargs: { loglevel: debug }

streamstarts: [ integerAddStream ]

eventstreams:

   integerAddStream:         
      - type: event
        agent: c_agent
        method: addInteger
        trigger: addDone
        args:
          a: 25
          b: 50

      - type: trigger
        triggers: [ {event: addDone, retVal: 75, target: 'integerSubtractStream'} ]

   integerSubtractStream:
      - type: event
        agent: c_agent
        method: subtractInteger
        trigger: subDone
        args:
          a: 50
          b: 25

      - type: trigger
        triggers: [ {event: subDone, retVal: 25, target: 'exit'} ]

   integerMultiplyStream:
      - type: event
        agent: c_agent
        method: multiplyInteger
        trigger: multDone
        args:
          a: 50
          b: 25

      - type: trigger
        triggers: [ {event: multDone, retVal: 1250, target: 'integerDivideStream'} 
  
   integerDivideStream:
      - type: event
        agent: c_agent
        method: divideInteger
        trigger: divDone
        args:
          a: 50
          b: 25

      - type: trigger
        triggers: [ {event: divDone, retVal: 2, target: 'exit'} 
  
